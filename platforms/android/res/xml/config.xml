<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coolappz.tracker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="LowLatencyAudio">
        <param name="android-package" value="com.rjfun.cordova.plugin.LowLatencyAudio" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <name>tracker</name>
    <description>
        low latency audio tracker test
    </description>
    <author email="support@bluebit.co.uk" href="http://www.bluebit.co.uk">
        Bluebit
    </author>
    <gap:plugin name="org.apache.cordova.media" />
    <gap:plugin name="com.rjfun.cordova.plugin.lowlatencyaudio" />
    <content src="index.html" />
    <access origin="*" />
</widget>
